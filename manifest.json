{
  "name": "Get That Path",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Easily copy the CSS of any web element to the clipboard just by point-and-click.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "author": "Alex Boboc",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
      "128": "icon.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}